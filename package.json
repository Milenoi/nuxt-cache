{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "stylelint": "stylelint --ignore-path .stylelintignore '**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --fix --ignore-path .stylelintignore '**/*.{vue,scss}'",
    "stylelint:staged": "stylelint --fix --ignore-path .stylelintignore",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lint:fix": "prettier --write --list-different . && eslint . --fix",
    "lint:staged": "yarn run lint:eslint && yarn run lint:prettier"
  },
  "dependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@mdi/font": "^7.4.47",
    "@nuxt/eslint": "^0.3.8",
    "@nuxtjs/stylelint-module": "^5.0.1",
    "@tanstack/vue-query": "^5.27.5",
    "@vueuse/router": "^10.9.0",
    "eslint": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-scope": "^7.1.1",
    "h3": "^1.11.1",
    "h3-compression": "^0.3.2",
    "husky": "^8.0.0",
    "nuxt": "^3.10.3",
    "postcss-html": "^1.5.0",
    "sass": "^1.71.1",
    "sharp": "^0.33.2",
    "stylelint": "^15.10.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.1.6",
    "vite-plugin-vuetify": "^2.0.2",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.6"
  },
  "devDependencies": {
    "@nuxt/image": "npm:@nuxt/image-edge@latest",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "optionalDependencies": {
    "lint-staged": "^13.2.2"
  },
  "packageManager": "yarn@4.1.0"
}
