{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "stylelint": "stylelint --ignore-path .stylelintignore '**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --fix --ignore-path .stylelintignore '**/*.{vue,scss}'",
    "stylelint:staged": "stylelint --fix --ignore-path .stylelintignore",
    "lint": "yarn lint:eslint",
    "lint:eslint": "eslint . --fix",
    "lint:fix": "yarn run lint:eslint",
    "lint:staged": "yarn run lint:eslint"
  },
  "devDependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@nuxt/eslint": "^0.7.5",
    "@nuxtjs/stylelint-module": "^5.0.1",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@tanstack/query-core": "^5.29.0",
    "@tanstack/query-persist-client-core": "^5.29.0",
    "@tanstack/query-sync-storage-persister": "^5.29.0",
    "@tanstack/vue-query": "^5.27.5",
    "@vueuse/router": "^12.4.0",
    "eslint": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-scope": "^8.2.0",
    "h3": "^1.11.1",
    "h3-compression": "^0.3.2",
    "husky": "^9.1.7",
    "nuxt": "^3.10.3",
    "postcss-html": "^1.5.0",
    "sass": "^1.71.1",
    "sharp": "^0.33.2",
    "stylelint": "^16.13.2",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.4.5",
    "vite": "^6.0.9",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-vuetify": "^2.0.2",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@nuxt/image": "npm:@nuxt/image-edge@latest",
    "vuetify": "^3.7.7"
  },
  "optionalDependencies": {
    "lint-staged": "^15.4.1"
  },
  "packageManager": "yarn@4.1.0"
